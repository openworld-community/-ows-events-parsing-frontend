{
    "name": "ows-events-api",
    "private": true,
    "type": "module",
    "packageManager": "npm@9.8.1",
    "scripts": {
        "dev": "nuxt dev",
        "build": "nuxt build",
        "preview": "nuxt preview",
        "typecheck": "nuxt typecheck",
        "lint": "eslint . --max-warnings=0",
        "check-format": "prettier . -c",
        "format": "npm run check-format -- -w",
        "postinstall": "nuxt prepare",
        "db-migrate": "drizzle-kit generate:sqlite && tsx ./server/database/migration.ts",
        "db-populate": "tsx ./server/database/populate.ts",
        "db-studio": "drizzle-kit studio"
    },
    "dependencies": {
        "@libsql/client": "^0.3.1",
        "@quasar/extras": "^1.16.5",
        "autoprefixer": "^10.4.14",
        "drizzle-orm": "^0.27.2",
        "nanoid": "^4.0.2",
        "nuxt": "^3.6.1",
        "postcss": "^8.4.25",
        "quasar": "^2.12.3",
        "tailwindcss": "^3.3.2"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.0.2",
        "@inquirer/checkbox": "^1.3.6",
        "@inquirer/confirm": "^2.0.7",
        "@nuxt/devtools": "latest",
        "@nuxt/eslint-config": "^0.1.1",
        "@nuxtjs/eslint-module": "^4.1.0",
        "@types/node": "^18",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "drizzle-kit": "^0.19.12",
        "eslint": "^8.44.0",
        "eslint-plugin-nuxt": "^4.0.0",
        "eslint-plugin-vue": "^9.15.1",
        "nuxt-quasar-ui": "^2.0.4",
        "prettier": "^2.8.8",
        "prettier-plugin-tailwindcss": "^0.3.0",
        "tsx": "^3.12.7",
        "typescript": "^5.1.6",
        "unplugin-icons": "^0.16.5",
        "vue-tsc": "^1.8.3"
    },
    "overrides": {
        "@inquirer/checkbox": {
            "figures": "^5.0.0"
        }
    }
}
